<view class="cont">
    <view class="panel">
        <view class="item">
            <text class="key">姓名</text>
            <text class="value">{{userInfoData.nickName}}</text>
        </view>
        <view class="item">
            <text class="key">年龄</text>
            <text class="value">{{userInfoData.age+'岁'}}</text>
        </view>
        <view class="item">
            <text class="key">账号</text>
            <text class="value">{{userInfoData.number}}</text>
        </view>
        <view class="item">
            <text class="key">职位</text>
            <text class="value">{{userInfoData.position}}</text>
        </view>
    </view>
</view>