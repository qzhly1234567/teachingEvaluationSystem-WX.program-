/**app.wxss**/
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
}
.panel {
  background: #fff;
  padding: 25rpx;
  border-radius: 20rpx;
  margin-top: 20rpx;
  transition: 0.2s;
}
.panel .panel_title {
  color: #0086F1;
  font-size: 28rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.panel:first-child {
  margin-top: 0px;
}
.err {
  background: #fb6155;
  text-align: center;
}
.err .panel_title {
  color: #fff;
}
.success {
  background: #228e33;
  text-align: center;
}
.success .panel_title {
  color: #fff;
}
.onTap:active {
  transform: scale(0.95);
}
page {
  width: 100%;
  height: 100%;
}
page .cont {
  width: 100%;
  height: 100%;
  background: #f0f2f5;
  padding: 0 30rpx;
  box-sizing: border-box;
  padding-top: 20rpx;
}
page .cont .panel {
  padding: 30rpx;
  display: flex;
  align-items: center;
  position: relative;
}
page .cont .panel image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin-right: 30rpx;
}
page .cont .panel .info text {
  display: block;
  font-size: 30rpx;
  color: #666;
}
page .cont .panel .info text:last-child {
  color: #999;
}
page .cont .panel .info .noEvaluation {
  color: #fb6155 !important;
}
page .cont .panel .info .alreadyEvaluation {
  color: #228e33 !important;
}
page .cont .panel .more {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: 30rpx;
  margin-right: 0;
}
page .cont .success {
  margin-top: 20rpx;
}
